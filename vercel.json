{
    "version": 2,
    "builds": [
      {
        "src": "keypoint_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "keypoint_project/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles"
  }