{
    "builds": [{
      "src": "keypoint_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb", 
        "runtime": "python3.12.4", 
        "buildCommand": "sh build.sh"
      }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "keypoint_project/wsgi.py"
      }
    ]
}